<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLargeFourVM" type="com.bcasapplication.app.modules.androidlargefour.data.viewmodel.AndroidLargeFourVM"/>
    </data>
    <LinearLayout android:id="@+id/linearAndroidLarge"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylelight_green_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowvectorOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_122pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVectorOne"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSetting"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_91pxh"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:singleLine="true"
                    android:text="@{androidLargeFourVM.androidLargeFourModel.txtSetting}"
                    style="@style/txtMavenProbold33" tools:text="@string/lbl_setting"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearColumnlockOne"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/_8pxh" android:paddingVertical="@dimen/_8pxv">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowlockOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_22"
                    android:id="@+id/btnLockOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_44pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_lock" tools:ignore="ContentDescription"/>
                <Spinner
                    android:background="@drawable/layer_list_bg_gray_50_border_gray_900_radius_7"
                    android:gravity="center_vertical"
                    android:id="@+id/spinnerChangeyour"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowmaterialsymb"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnmaterials"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_484pxv"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_22"
                        android:id="@+id/btnMaterialsymbol"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_width="@dimen/_44pxh"
                        android:paddingHorizontal="@dimen/_9pxh"
                        android:paddingVertical="@dimen/_9pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_material_symbol" tools:ignore="ContentDescription"/>
                    <ImageButton
                        android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_22"
                        android:id="@+id/btnIconbutton"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_44pxh"
                        android:paddingHorizontal="@dimen/_10pxh"
                        android:paddingVertical="@dimen/_10pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_noto_star" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnview"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackabouttheap"
                        android:layout_height="@dimen/_220pxv" android:layout_width="@dimen/_285pxh">
                        <LinearLayout
                            android:id="@+id/linearColumnaboutthea"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_11pxh"
                            android:paddingStart="@dimen/_11pxh"
                            android:paddingTop="@dimen/_6pxv" style="@style/groupStylegray_50cornerRadius">
                            <TextView android:id="@+id/txtAbouttheapp"
                                android:layout_height="match_parent"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtAbouttheapp}"
                                style="@style/txtHindMysurubold16_1" tools:text="@string/lbl_about_the_app"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearColumngivefeedb"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_82pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingBottom="@dimen/_7pxv"
                            android:paddingEnd="@dimen/_9pxh"
                            android:paddingStart="@dimen/_9pxh"
                            android:paddingTop="@dimen/_7pxv" style="@style/groupStylegray_50cornerRadius">
                            <TextView android:id="@+id/txtGivefeedback"
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtGivefeedback}"
                                style="@style/txtHindMysurubold16_1" tools:text="@string/lbl_give_feedback"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearColumnchangeyou"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv" style="@style/groupStylegray_1">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtChangeyourOne"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtChangeyourOne}"
                                style="@style/txtHindMysurubold15" tools:text="@string/msg_change_your_name"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_4"
                                android:id="@+id/viewView"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_25pxv" android:layout_width="@dimen/_250pxh"/>
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtChangeyourTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_9pxv"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtChangeyourTwo}"
                                style="@style/txtHindMysurubold15" tools:text="@string/msg_change_your_phone"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_4"
                                android:id="@+id/viewViewOne"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_25pxv" android:layout_width="@dimen/_250pxh"/>
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtChangeyour"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="match_parent"
                                android:letterSpacing="0.02"
                                android:singleLine="true"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtChangeyour}"
                                style="@style/txtHindMysurubold15" tools:text="@string/msg_change_your_relative_s"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_50_border_gray_900_radius_4"
                                android:id="@+id/viewViewTwo"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_25pxv" android:layout_width="@dimen/_250pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSave"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="wrap_content"
                                android:letterSpacing="0.02"
                                android:paddingEnd="@dimen/_15pxh"
                                android:paddingStart="@dimen/_15pxh"
                                android:singleLine="true"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtSave}"
                                style="@style/txtSolidRoundedOutline" tools:text="@string/lbl_save"/>
                        </LinearLayout>
                    </FrameLayout>
                    <View android:id="@+id/viewViewThree"
                        android:layout_height="@dimen/_386pxh" android:layout_width="@dimen/_108pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_black_900_radius_2"
                        android:id="@+id/lineLine"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_66pxh" android:layout_width="@dimen/_108pxh"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
